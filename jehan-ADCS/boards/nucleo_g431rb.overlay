#include <dt-bindings/pinctrl/stm32-pinctrl.h>

&pinctrl{
	usart1_init_high: usart1_init_high {
		pinmux = <STM32_PINMUX('C', 4, AF7)>;
		bias-pull-up;
	};
	usart1_rx_pb6: usart1_rx_pb6 {
		pinmux = <STM32_PINMUX('B', 6, AF7)>;
	};

	usart1_tx_pb7: usart1_tx_pb7 {
		pinmux = <STM32_PINMUX('B', 7, AF7)>;
	};

};

&usart1 {
	pinctrl-0 = <&usart1_init_high>;
	pinctrl-1 = <&usart1_tx_pb7 &usart1_rx_pb6>;
	pinctrl-names = "idle", "default";
	current-speed = <115200>;
	status = "okay";
};
